@model IEnumerable<BitirmeProje.Models.UYELER>

@{
    ViewBag.Title = "uyesilkayıt";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>uyesilkayıt</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-active">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UYEAD)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UYESOYAD)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UYEYETKI)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KADI)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SIFRE)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UYEAD)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UYESOYAD)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UYEYETKI)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.KADI)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SIFRE)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
